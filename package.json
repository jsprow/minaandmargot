{
  "name": "minaandmargot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dr-kobros/react-webfont-loader": "^8.0.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "nuka-carousel": "^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.5.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components -o src/components/css",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components -o src/components/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
